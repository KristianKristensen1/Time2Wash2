<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.krist.time2washproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/vaskemaskiner"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterUserActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".myBookingPopup" android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".BookTimePopup" android:theme="@style/AppTheme.CustomTheme" />
        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true"></service>
        <service android:name=".ProgressBarService"
            android:enabled="true">
        </service>

<receiver android:name=".AlertReceiver"/>
    </application>

</manifest>